<?xml version="1.0" encoding="UTF-8"?>
<mapcache>


  <tileset name="NLCDTestTiles">
    <metadata>
      <title>NLCDTestTiles</title>
    </metadata>
    <source>NLCDTestTiles</source>
    <cache>agolCompatible</cache>
    <format>PNG</format>
    <grid>GoogleMapsCompatible</grid>
    <metatile>1 1</metatile>
    <expires>1</expires>
  </tileset>

  <source name="NLCDTestTiles" type="wms">
    <getmap>
      <params>
        <FORMAT>image/png</FORMAT>
        <LAYERS>NLCDTestTiles</LAYERS>
        <TRANSPARENT>TRUE</TRANSPARENT>
      </params>
    </getmap>
    <http>
      <url>10.10.0.3/nfwf/?map=/map/nfwf-conus-ncld-test.map</url>
    </http>
  </source>

  <service type="wms" enabled="true">
    <resample_mode>bilinear</resample_mode>
  </service>

  <cache name="agolCompatible" type="disk" layout="template">
    <template>/var/www/html/mapcache/cache/{tileset}/{z}/{x}/{inv_y}.{ext}</template>
    <detect_blank/>
  </cache>

  <cache name="tmpl" type="disk" layout="template">
    <template>/var/www/html/mapcache/cache/{tileset}/{z}/{x}/{y}.{ext}</template>
    <detect_blank/>
  </cache>

  <service type="wmts" enabled="true"/>
  <service type="tms" enabled="true"/>
  <service type="demo" enabled="true"/>

  <log_level>debug</log_level>

</mapcache>
