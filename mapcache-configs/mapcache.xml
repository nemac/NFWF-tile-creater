<?xml version="1.0" encoding="UTF-8"?>
<mapcache>

  <cache name="tmpl" type="disk" layout="template">
    <template>/var/www/html/mapcache/cache/{tileset}/{z}/{x}/{y}.{ext}</template>
    <detect_blank/>
  </cache>

  <source name="ExposureIndexTilesTest" type="wms">
    <getmap>
      <params>
        <FORMAT>image/png</FORMAT>
        <LAYERS>ExposureIndexTilesTest</LAYERS>
        <TRANSPARENT>TRUE</TRANSPARENT>
      </params>
    </getmap>

    <http>
      <url>10.10.0.3/nfwf/?map=/map/nfwf.map</url>
    </http>
  </source>

  <tileset name="ExposureIndexTilesTest">
    <metadata>
      <title>ExposureIndexTilesTest</title>
    </metadata>
    <source>ExposureIndexTilesTest</source>
    <cache>tmpl</cache>
    <format>PNG</format>
    <grid>GoogleMapsCompatible</grid>
    <metatile>1 1</metatile>
    <expires>1</expires>
  </tileset>

  <source name="AssetsIndexTiles" type="wms">
    <getmap>
      <params>
        <FORMAT>image/png</FORMAT>
        <LAYERS>AssetsIndexTiles</LAYERS>
        <TRANSPARENT>TRUE</TRANSPARENT>
      </params>
    </getmap>

    <http>
      <url>10.10.0.3/nfwf/?map=/map/nfwf.map</url>
    </http>
  </source>

  <tileset name="AssetsIndexTiles">
    <metadata>
      <title>AssetsIndexTiles</title>
    </metadata>
    <source>AssetsIndexTiles</source>
    <cache>tmpl</cache>
    <format>PNG</format>
    <grid>GoogleMapsCompatible</grid>
    <metatile>1 1</metatile>
    <expires>1</expires>
  </tileset>

  <format name="PNG_BEST" type ="PNG">
    <compression>fast</compression>
  </format>

